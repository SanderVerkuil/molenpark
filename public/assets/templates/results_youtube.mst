<table class="table table-striped table-hover">
  {{#results}}
  <tr data-id="{{videoId}}" title="Klik om te selecteren">
    <td class="video-thumbnail hidden-sm">
      {{#thumbnails}}
        {{#medium}}
          <img src="{{url}}" />
        {{/medium}}
      {{/thumbnails}}
    </td>
    <td class="video-info">
      <span class="video-title">{{title}}</span>
    </td>
    <td>
      <button class="btn btn-danger btn-play" title="Voorluisteren">
        <i class="glyphicon glyphicon-play"></i>
      </button>
    </td>
  </tr>
  {{/results}}
</table>
